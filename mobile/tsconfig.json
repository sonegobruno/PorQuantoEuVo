{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "jsx": "react-native",
      "lib": ["dom", "esnext"],
      "moduleResolution": "node",
      "noEmit": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "baseUrl": ".",
      "paths": {
        "*": ["src/*"]
      }
    },
    "extends": "expo/tsconfig.base",
}
